public class CalendarPrinter {
public static void main (String[] args) {
	int d = 1;
	String month = "September ";
	String weekend = " - WEEKEND";
	String labor = " - NO SCHOOL (LABOR DAY)";
	String mark = " - END OF THE MARKING PERIOD";
	while (d<=30) 
	{
		System.out.println (month + d + ((d == 3 || d == 4 || d == 10 || d == 11 || d == 17 || d == 18 || d == 24 || d == 25) ? weekend : ((d == 5) ? labor : d == 23 ? mark : "")));
		d++;
        }

    }
}
